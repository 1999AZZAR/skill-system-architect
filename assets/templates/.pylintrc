[MASTER]
jobs=0
suggestion-mode=yes

[MESSAGES CONTROL]
disable=C0114,C0115,C0116

[FORMAT]
max-line-length=88
indent-string='    '

[DESIGN]
max-args=7
max-attributes=7
max-bool-expr=5
max-branches=12
max-locals=15
max-returns=6
max-statements=50

[BASIC]
good-names=i,j,k,ex,Run,_,id,db

[TYPECHECK]
generated-members=

[SIMILARITIES]
min-similarity-lines=4
ignore-imports=yes
